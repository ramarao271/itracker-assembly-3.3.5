            </field>
        </form>
        <!-- check this; this was done fast... -->
        <form name="displayReportForm">
            <field property="type">
            </field>
            <field property="projectIds">
            </field>
            <field property="reportId">
            </field>
            <field property="reportOutput">
            </field>
            <field property="map">
            </field>
        </form>
    </formset>

</form-validation>
